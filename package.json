{
  "name": "cmd-v2-fed3-toets1",
  "version": "1.0.0",
  "description": "toets 1 voor het vak frontend 3",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run transpile:js && npm run min && npm run copy-rest",
    "transpile:js": "babel src -d dist",
    "min": "npm run min:html & npm run min:css",
    "min:html": "minify src/index.html > dist/index.html",
    "min:css": "minify src/style.css > dist/style.css",
    "copy-rest": "ncp src/android-app-data.csv dist/android-app-data.csv"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morkvd/CMD-V2-FED3-toets1.git"
  },
  "keywords": [
    "CMD"
  ],
  "author": "markvdijken@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/morkvd/CMD-V2-FED3-toets1/issues"
  },
  "homepage": "https://github.com/morkvd/CMD-V2-FED3-toets1#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "minify": "^2.0.11",
    "ncp": "^2.0.0"
  }
}
